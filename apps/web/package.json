{
  "name": "@baileapp/web",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:staging": "vite --mode staging",
    "build": "vite build",
    "build:staging": "vite build --mode staging",
    "preview": "vite preview --port 4173",
    "preview:staging": "vite preview --mode staging --port 4173",
    "lint": "eslint --ext .ts,.tsx src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "check:auth": "powershell -Command \"if (Select-String -Path 'src\\**\\*.ts','src\\**\\*.tsx' -Pattern 'useAuth\\(\\)' | Select-String -Pattern '(utils|stores|services)') { Write-Host '❌ useAuth usado fuera de componentes'; exit 1 } else { Write-Host '✅ useAuth sólo en componentes' }\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.86.0",
    "@tanstack/react-query": "^5.90.11",
    "date-fns": "^4.1.0",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.23.25",
    "ics": "^3.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "tslib": "^2.8.1",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tanstack/react-query-devtools": "^5.91.1",
    "@types/file-saver": "^2.0.7",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.7.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.6"
  }
}
