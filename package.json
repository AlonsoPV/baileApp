{
  "name": "baileapp",
  "private": true,
  "version": "0.0.1",
  "main": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "start": "expo start",
    "start:tunnel": "expo start --tunnel",
    "dev:web": "pnpm --filter @baileapp/web dev",
    "build:web": "pnpm --filter @baileapp/web build",
    "preview:web": "pnpm --filter @baileapp/web preview",
    "eas-build-post-install": "bash ci_scripts/ensure_pods.sh || true",
    "build:dev:android": "eas build --profile developmentClient --platform android",
    "build:dev:ios": "eas build --profile developmentClient --platform ios",
    "build:preview:android": "eas build --profile preview --platform android",
    "build:preview:ios": "eas build --profile preview --platform ios",
    "build:prod:android": "eas build --profile production --platform android",
    "build:prod:ios": "eas build --profile production --platform ios",
    "submit:android": "eas submit --platform android --profile production",
    "submit:ios": "eas submit --platform ios --profile production",
    "update": "eas update --branch production --message \"OTA\"",
    "update:android": "eas update --branch production --message \"OTA\" --platform android",
    "update:ios": "eas update --branch production --message \"OTA\" --platform ios",
    "fix:certificates": "powershell -ExecutionPolicy Bypass -File scripts/actualizar-certificados-nodejs.ps1",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^7.13.0",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.12.0",
    "@supabase/supabase-js": "^2.95.3",
    "@tanstack/react-query": "^5.90.21",
    "dotenv": "^16.6.1",
    "expo": "^54.0.33",
    "expo-constants": "^18.0.13",
    "expo-file-system": "~19.0.21",
    "expo-linking": "^8.0.11",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "^29.0.16",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-webview": "13.16.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "babel-preset-expo": "^54.0.10",
    "eas-cli": "16.28.0"
  },
  "packageManager": "pnpm@10.24.0"
}
