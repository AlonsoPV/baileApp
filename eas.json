{
  "cli": {
    "version": ">= 16.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true,
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1"
        }
      }
    },
    "developmentClient": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development",
      "ios": {
        "simulator": true,
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1"
        }
      }
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "ios": {
        "simulator": false,
        "buildConfiguration": "Release",
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1"
        }
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "latest",
        "node": "20.19.4",
        "env": {
          "YARN_IGNORE_ENGINES": "1"
        }
      },
      "ios": {
        "image": "latest",
        "node": "20.19.4",
        "buildConfiguration": "Release",
        "env": {
          "EXPO_NO_CAPABILITY_SYNC": "1"
        }
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json"
      },
      "ios": {
        "appleId": "alpeva96@gmail.com"
      }
    }
  }
}

