UPLOAD_STORE_FILE=upload-keystore.jks
UPLOAD_STORE_PASSWORD=SET_IN_LOCAL_ENV
UPLOAD_KEY_ALIAS=upload
UPLOAD_KEY_PASSWORD=SET_IN_LOCAL_ENV
